{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base", 
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "lines-between-class-members": ["error", "always", { "exceptAfterSingleLine": true }]
           
     }
}
